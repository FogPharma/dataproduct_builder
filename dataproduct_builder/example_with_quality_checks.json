{
  "input_path": "data/input.csv",
  "output_path": "data/output.csv",
  "steps": [
    {
      "operation": "check_row_count",
      "params": {
        "min_count": 10
      }
    },
    {
      "operation": "check_column_count",
      "params": {
        "min_count": 3
      }
    },
    {
      "operation": "check_missing_values",
      "params": {
        "threshold": 0.05
      }
    },
    {
      "operation": "drop_duplicates",
      "params": {
        "subset": ["id"],
        "keep": "first"
      }
    },
    {
      "operation": "filter_rows",
      "params": {
        "column": "score",
        "operator": ">=",
        "value": 0.5
      }
    },
    {
      "operation": "check_row_count",
      "params": {
        "min_count": 5
      }
    },
    {
      "operation": "sort_rows",
      "params": {
        "by": "score",
        "ascending": false
      }
    }
  ]
}
